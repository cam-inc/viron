{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "clean": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".cache/**", ".docusaurus/**"]
    },
    "@viron/app#dev": {
      "with": ["//#example"],
      "dependsOn": ["@viron/linter#build", "@viron/lib#build"],
      "cache": false
    },
    "//#example": {
      "cache": false
    },
    "format": {},
    "lint": {},
    "check-types": {},
    "test": {},
    "pre-push": {}
  }
}
