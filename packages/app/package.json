{
  "name": "@viron/app",
  "version": "2.15.0",
  "engines": {
    "node": ">=18.14.0",
    "npm": ">=9.3.1"
  },
  "browserslist": [
    ">5%",
    "last 2 versions",
    "not dead"
  ],
  "scripts": {
    "pre-push": "npm run ci:local",
    "dev": "gatsby develop --https -p 8000 --host local.viron.work --open",
    "build": "gatsby build",
    "build:prefix": "gatsby build --prefix-paths",
    "serve": "gatsby serve -p 9000",
    "launch": "npm run build && npm run serve",
    "clean": "gatsby clean",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:record": "cypress run --record",
    "test:unit": "jest --passWithNoTests",
    "check-types": "tsc",
    "ci:local": "START_SERVER_AND_TEST_INSECURE=1 CYPRESS_BASE_URL=http://localhost:9000 start-server-and-test launch http://localhost:9000 test:e2e",
    "ci:local:record": "START_SERVER_AND_TEST_INSECURE=1 CYPRESS_BASE_URL=http://localhost:9000 start-server-and-test launch http://localhost:9000 test:e2e:record",
    "ci:development": "CYPRESS_BASE_URL=https://viron.work npm run test:e2e",
    "ci:development:record": "CYPRESS_BASE_URL=https://viron.work npm run test:e2e:record",
    "ci:production": "CYPRESS_BASE_URL=https://viron.app npm run test:e2e",
    "ci:production:record": "CYPRESS_BASE_URL=https://viron.app npm run test:e2e:record",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint"
    ]
  },
  "dependencies": {
    "@ark-ui/react": "^0.13.1",
    "@editorjs/editorjs": "^2.22.1",
    "@heroicons/react": "^1.0.5",
    "@hookform/resolvers": "^2.9.11",
    "@radix-ui/react-accordion": "^1.2.4",
    "@radix-ui/react-dialog": "^1.1.7",
    "@radix-ui/react-dropdown-menu": "^2.1.7",
    "@radix-ui/react-label": "^2.1.3",
    "@radix-ui/react-popover": "^1.1.7",
    "@radix-ui/react-select": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.3",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-tooltip": "^1.2.0",
    "@react-icons/all-files": "^4.1.0",
    "@tailwindcss/aspect-ratio": "^0.2.1",
    "@tailwindcss/container-queries": "^0.1.1",
    "@viron/linter": "*",
    "babel-jest": "^27.0.6",
    "babel-preset-gatsby": "^3.11.0",
    "browser-lang": "^0.2.1",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.2.6",
    "clsx": "^2.1.1",
    "color-blend": "^3.0.1",
    "gatsby": "^5.11.0",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-lodash": "^6.11.0",
    "gatsby-plugin-page-creator": "^5.11.0",
    "gatsby-plugin-postcss": "^6.11.0",
    "gatsby-plugin-react-helmet-async": "^1.2.3",
    "gatsby-plugin-react-i18next": "^3.0.1",
    "gatsby-source-filesystem": "^5.11.0",
    "i18next": "^22.5.0",
    "i18next-browser-languagedetector": "^6.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "jsonpath-plus": "^5.0.7",
    "lodash": "^4.17.21",
    "lucide-react": "^0.488.0",
    "markdown-it": "^12.2.0",
    "postcss": "^8.4.31",
    "postcss-preset-env": "^7.0.1",
    "qrcode": "^1.5.1",
    "query-string": "^6.14.1",
    "react": "^18.3.1",
    "react-dnd": "^14.0.3",
    "react-dnd-html5-backend": "^14.0.1",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.55.0",
    "react-i18next": "^12.3.1",
    "react-test-renderer": "^18.2.0",
    "recoil": "^0.7.7",
    "sortablejs": "^1.15.0",
    "store2": "^2.12.0",
    "swr": "^1.1.0",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^3.4.17",
