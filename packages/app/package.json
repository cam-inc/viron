{
  "name": "@viron/app",
  "version": "2.0.0",
  "engines": {
    "node": ">=15.13.0",
    "npm": ">=7.9.0"
  },
  "scripts": {
    "pre-commit": "run-s lint:staged test:static",
    "pre-push": "npm run test:unit",
    "preflight": "npm install --legacy-peer-deps",
    "develop": "gatsby develop --https -p 8000",
    "build": "gatsby build",
    "build:prefix": "gatsby build --prefix-paths",
    "serve": "gatsby serve -p 9000",
    "serve:prefix": "gatsby serve -p 9000 --prefix-paths",
    "launch": "run-s build serve",
    "launch:prefix": "run-s build:prefix serve:prefix",
    "clean": "gatsby clean",
    "format": "run-p format:*",
    "format:tjs": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "lint": "npm run lint:tjs",
    "lint:tjs": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "lint:staged": "lint-staged",
    "test": "run-s test:static test:unit test:e2e",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:record": "cypress run --record",
    "test:unit": "jest",
    "test:static": "tsc",
    "ci:local": "START_SERVER_AND_TEST_INSECURE=1 CYPRESS_BASE_URL=http://localhost:9000 start-server-and-test launch http://localhost:9000 test:e2e",
    "ci:local:record": "START_SERVER_AND_TEST_INSECURE=1 CYPRESS_BASE_URL=http://localhost:9000 start-server-and-test launch http://localhost:9000 test:e2e:record",
    "ci:development": "CYPRESS_BASE_URL=https://viron.work npm run test:e2e",
    "ci:development:record": "CYPRESS_BASE_URL=https://viron.work npm run test:e2e:record",
    "ci:production": "CYPRESS_BASE_URL=https://viron.app npm run test:e2e",
    "ci:production:record": "CYPRESS_BASE_URL=https://viron.app npm run test:e2e:record",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint"
    ]
  },
  "dependencies": {
    "@editorjs/editorjs": "^2.22.1",
    "@hookform/resolvers": "^2.0.0",
    "@reach/router": "^1.3.4",
    "@react-icons/all-files": "^4.1.0",
    "@tailwindcss/aspect-ratio": "^0.2.1",
    "@viron/linter": "*",
    "babel-jest": "^27.0.6",
    "babel-preset-gatsby": "^1.9.0",
    "classnames": "^2.2.6",
    "gatsby": "^3.10.0",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-lodash": "^4.0.0",
    "gatsby-plugin-page-creator": "^3.0.0",
    "gatsby-plugin-postcss": "^4.0.0",
    "gatsby-plugin-react-helmet-async": "^1.2.0",
    "i18next": "^20.0.0",
    "i18next-browser-languagedetector": "^6.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "jsonpath-plus": "^5.0.7",
    "lodash": "^4.17.21",
    "markdown-it": "^12.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.7",
    "postcss-preset-env": "^6.7.0",
    "qrcode": "^1.4.4",
    "query-string": "^6.14.1",
    "react": "^17.0.2",
    "react-dnd": "^14.0.3",
    "react-dnd-html5-backend": "^14.0.1",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.0.9",
    "react-hook-form": "^7.0.2",
    "react-i18next": "^11.8.10",
    "react-test-renderer": "^17.0.2",
    "recoil": "^0.1.3",
    "store2": "^2.12.0",
    "tailwindcss": "^2.2.2",
    "webpack": "^5.24.4",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@hookform/devtools": "^3.0.1",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/react": "^6.1.21",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^26.0.24",
    "@types/qrcode": "^1.4.0",
    "@types/react-dom": "^17.0.2",
    "@types/react-test-renderer": "^17.0.1",
    "@types/recoil": "^0.0.1",
    "@types/tailwindcss": "^2.0.6",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "babel-loader": "^8.2.2",
    "babel-preset-react-app": "^10.0.0",
    "cypress": "^8.0.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gatsby-cypress": "^1.9.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "start-server-and-test": "^1.12.6",
    "ts-jest": "^27.0.3",
    "typescript": "^4.2.3"
  }
}
